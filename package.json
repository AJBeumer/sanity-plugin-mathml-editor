{
  "name": "sanity-plugin-mathml-editor",
  "version": "0.1.0",
  "description": "Visual MathML + LaTeX editor for Sanity using MathLive",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "prepare": "npm run build"
  },
  "peerDependencies": {
    "sanity": "^3.0.0 || ^4.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "@sanity/ui": "^1.0.0",
    "mathlive": "^0.99.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "sanity",
    "sanity-plugin",
    "math",
    "mathlive",
    "latex",
    "mathml",
    "editor"
  ],
  "author": "AJ Beumer",
  "license": "MIT"
}
